{% extends 'yascms:themes/' ~ request.effective_theme_name ~ '/macros.jinja2' %}
{% block first_page_url %}{{ request.route_url('search_results', _query={'p': 1, 'q': quantity_per_page, 'c': request.GET['c']}) }}{% endblock %}
{% block next_page_url %}{{ request.route_url('search_results', _query={'p': current_page_number+1, 'q': quantity_per_page, 'c': request.GET['c']}) }}{% endblock %}
{% block prev_page_url %}{{ request.route_url('search_results', _query={'p': current_page_number-1, 'q': quantity_per_page, 'c': request.GET['c']}) }}{% endblock %}
{% block last_page_url %}{{ request.route_url('search_results', _query={'p': quantity_of_pages, 'q': quantity_per_page, 'c': request.GET['c']}) }}{% endblock %}
