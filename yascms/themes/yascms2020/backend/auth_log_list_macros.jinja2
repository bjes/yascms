{% extends 'yascms:themes/' ~ request.effective_theme_name ~ '/macros.jinja2' %}
{% if request.GET['u'] %}
  {% block first_page_url %}{{ request.route_url('backend_auth_log_list', _query={'p': 1, 'q': quantity_per_page, 'u': request.GET['u']}) }}{% endblock %}
  {% block next_page_url %}{{ request.route_url('backend_auth_log_list', _query={'p': current_page_number+1, 'q': quantity_per_page, 'u': request.GET['u']}) }}{% endblock %}
  {% block prev_page_url %}{{ request.route_url('backend_auth_log_list', _query={'p': current_page_number-1, 'q': quantity_per_page, 'u': request.GET['u']}) }}{% endblock %}
  {% block last_page_url %}{{ request.route_url('backend_auth_log_list', _query={'p': quantity_of_pages, 'q': quantity_per_page, 'u': request.GET['u']}) }}{% endblock %}
{% endif %}
